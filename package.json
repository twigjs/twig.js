{
  "author": "John Roepke <john@justjohn.us> (http://john.sh/)",
  "name": "twig",
  "description": "JS port of the Twig templating language.",
  "version": "1.17.1",
  "homepage": "https://github.com/twigjs/twig.js",
  "license": "BSD-2-Clause",
  "licenses": [
    {
      "type": "BSD-2-Clause",
      "url": "https://raw.github.com/twigjs/twig.js/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/twigjs/twig.js.git"
  },
  "main": "src/twig.js",
  "engines": {
    "node": ">=12"
  },
  "bin": {
    "twigjs": "./bin/twigjs"
  },
  "scripts": {
    "preversion": "npm test && git diff --exit-code --quiet",
    "postversion": "git push origin master && git push origin master --tags",
    "pretest": "npm run build",
    "test": "mocha -r should",
    "build": "node node/build.js",
    "posttest": "xo src lib bin"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "locutus": "^2.0.11",
    "minimatch": "3.0.x",
    "walk": "2.3.x"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "eslint-plugin-mocha": "^6.3.0",
    "mocha": "^11.1.0",
    "path-browserify": "^1.0.1",
    "should": "^13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "^9.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "tokenizer2": "^2.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "xo": "^0.39.1"
  },
  "browser": {
    "fs": false
  },
  "xo": {
    "space": 4,
    "envs": [
      "browser",
      "node",
      "mocha"
    ],
    "plugins": [
      "mocha"
    ],
    "rules": {
      "block-scoped-var": 0,
      "brace-style": 0,
      "comma-dangle": 0,
      "complexity": 0,
      "default-case": 0,
      "dot-notation": 0,
      "eslint-comments/no-unused-disable": 0,
      "func-call-spacing": 0,
      "func-name-matching": 0,
      "func-names": 0,
      "guard-for-in": 0,
      "handle-callback-err": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-self-import": 0,
      "indent": 0,
      "key-spacing": 0,
      "max-depth": 0,
      "max-params": 0,
      "max-statements-per-line": 0,
      "mocha/prefer-arrow-callback": 0,
      "new-cap": 0,
      "no-alert": 0,
      "no-caller": 0,
      "no-cond-assign": 0,
      "no-constant-condition": 0,
      "no-control-regex": 0,
      "no-empty": 0,
      "no-eq-null": 0,
      "no-eval": 0,
      "no-extend-native": 0,
      "no-global-assign": 0,
      "no-implicit-coercion": 0,
      "no-misleading-character-class": 0,
      "no-mixed-operators": 0,
      "no-mixed-spaces-and-tabs": 0,
      "no-multi-assign": 0,
      "no-negated-condition": 0,
      "no-new": 0,
      "no-new-func": 0,
      "no-new-object": 0,
      "no-path-concat": 0,
      "no-proto": 0,
      "no-prototype-builtins": 0,
      "no-redeclare": 0,
      "no-return-assign": 0,
      "no-self-compare": 0,
      "no-sequences": 0,
      "no-shadow-restricted-names": 0,
      "no-throw-literal": 0,
      "no-undef": 0,
      "no-unexpected-multiline": 0,
      "no-unused-expressions": 0,
      "no-use-extend-native/no-use-extend-native": 0,
      "no-useless-call": 0,
      "no-useless-escape": 0,
      "no-var": 0,
      "no-void": 0,
      "no-warning-comments": 0,
      "node/no-deprecated-api": 0,
      "object-curly-spacing": 0,
      "prefer-arrow-callback": 0,
      "prefer-const": 0,
      "prefer-destructuring": 0,
      "prefer-rest-params": 0,
      "prefer-spread": 0,
      "promise/prefer-await-to-then": 0,
      "quote-props": 0,
      "quotes": 0,
      "radix": 0,
      "semi-style": 0,
      "space-in-parens": 0,
      "unicorn/error-message": 0,
      "unicorn/escape-case": 0,
      "unicorn/explicit-length-check": 0,
      "unicorn/filename-case": 0,
      "unicorn/no-nested-ternary": 0,
      "unicorn/prefer-node-append": 0,
      "unicorn/prefer-string-slice": 0,
      "unicorn/prevent-abbreviations": 0
    }
  }
}
